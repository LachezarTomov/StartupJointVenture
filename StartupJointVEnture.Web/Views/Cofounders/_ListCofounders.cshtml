@model IEnumerable<StartupJointVenture.Web.ViewModels.CofounderViewModel>

@foreach (var item in Model)
{
    <div class="row panel">
        @if (item.Author.ImageUrl == null)
        {
            <img src="~/Content/images/nopicture.png" />
        }
        else
        {
            <img src="@item.Author.ImageUrl" />
        }
        <strong>
            @item.Author.FirstName
            @item.Author.LastName
        </strong>
        (@item.Location)
        <div>
            <em>@item.ProfSkills </em>
        </div>
    </div>
}
