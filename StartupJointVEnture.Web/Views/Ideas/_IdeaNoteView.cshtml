@model IEnumerable<StartupJointVenture.Models.Idea>

@foreach (var idea in Model)
{
    <div class="col-sm-4 col-lg-4 col-md-4">
        <div class="quote-container">
            <span class="pin"></span>
            <blockquote class="note yellow-note">
                @Html.ActionLink(idea.Title,"#")
            </blockquote>
        </div>
</div>
}
